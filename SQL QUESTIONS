    {
      "cell_type": "code",
      "source": [
        "SELECT Fruit, SUM(Quantity) AS Total_Quantity\n",
        "FROM Sales\n",
        "GROUP BY Fruit\n",
        "ORDER BY Total_Quantity ASC;\n"
      ],
      "metadata": {
        "id": "vR0F_0vVxEcW"
      },
